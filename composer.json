{
  "name": "oanhnn/laravel-webhook-notification",
  "type": "library",
  "description": "Integrate Webhook notification into Laravel 5.5+",
  "keywords": [
    "laravel",
    "package",
    "webhook",
    "notification"
  ],
  "license": "MIT",
  "homepage": "https://github.com/oanhnn/laravel-webhook-notification",
  "support": {
    "issues": "https://github.com/oanhnn/laravel-webhook-notification/issues",
    "source": "https://github.com/oanhnn/laravel-webhook-notification"
  },
  "authors": [
    {
      "name": "Oanh Nguyen",
      "email": "oanhnn.bk@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Laravel\\WebhookNotification\\": "src/"
    }
  },
  "require": {
    "php": "^7.1.3",
    "guzzlehttp/guzzle":"^6.3",
    "illuminate/notifications": "^5.5",
    "illuminate/support": "^5.5",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "mockery/mockery": "^0.9",
    "orchestra/testbench": "^3.5",
    "phpunit/phpunit": "~6.1|~7.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "scripts": {
    "phpunit": "php vendor/phpunit/phpunit/phpunit --coverage-html storage/coverage",
    "phpcs": "php vendor/squizlabs/php_codesniffer/bin/phpcs",
    "phpcbf": "php vendor/squizlabs/php_codesniffer/bin/phpcbf"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
